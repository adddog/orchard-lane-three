{
  presets: [
    [
      "es2015",
      {
        "loose": true
      }
    ], "stage-0", "stage-1", "stage-2"
  ],
  "plugins": [
    "transform-decorators-legacy",
    "external-helpers",
    ["transform-runtime",
    {
      "helpers": true,
      "polyfill": false,
      "regenerator": false,
      "moduleName": "babel-runtime"
    }]
  ]
}